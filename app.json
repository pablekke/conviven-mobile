{
  "expo": {
    "name": "expo-conviven",
    "slug": "expo-conviven",
    "version": "1.0.0",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "scheme": "conviven",
    "extra": {
      "router": {},
      "eas": {
        "projectId": "19e28da3-43e7-4595-a8bd-dd1dcfcf489d"
      }
    },
    "ios": {
      "bundleIdentifier": "com.conviven.expoconviven",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        }
      }
    }
  }
}
